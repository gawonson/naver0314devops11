package day0325;



public class Shop {
	private String sangpum;
	private int su;
	private int dan;
	
	public Shop(String sangpum, int su,int dan) {
		this.sangpum=sangpum;
		this.su=su;
		this.dan=dan;
	}
	public void setSangpum(String sangpum) {
		this.sangpum=sangpum;
	}
	public void setDan(int dan) {
		this.dan=dan;
	}
	public void setSu(int su) {
		this.su=su;
	}
	public String getSangpum()
	{
		return sangpum;
	}
	public int getSu() {
		return su;			
	}
	public int getDan() {
		return dan;
	}

	
}
