package day0315;

public class Ex1_DataType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		boolean flag=true;
		boolean flag2;
		flag2=false;
		
		System.out.println(flag);
		System.out.println(flag2);
		System.out.println(!flag);
		System.out.println(!flag2);
		
		System.out.println(flag && flag2);
		System.out.println(flag||flag2);

	}

}
