package day0325;



public class Shop {
	private String Sang;
	private int su;
	private int dan;
	
	public Shop(String Sang, int su,int dan) {
		this.Sang=Sang;
		this.su=su;
		this.dan=dan;
	}
	public void setSang(String Sang) {
		this.Sang=Sang;
	}
	public void setDan(int dan) {
		this.dan=dan;
	}
	public void setSu(int su) {
		this.su=su;
	}
	public String getSang()
	{
		return Sang;
	}
	public int getSu() {
		return su;			
	}
	public int getDan() {
		return dan;
	}

	
}
