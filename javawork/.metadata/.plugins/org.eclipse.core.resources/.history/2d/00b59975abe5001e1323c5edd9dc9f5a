package day0319;

public class Ex1_Munje {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 조건이 아닐때 밑으로 내려감
		//1-100사이의 소수를 모두 출력하고 몇개인지 갯수를 구하라
		loop:
		for (int i=2;i<=100;i++) {
			for (int j=2;j<i;j++) {
				if (i%j==0) {
					continue loop;
				}
				System.out.println(i);
			}
		}
	
		
		
	}

}
