package day0325;



public class Shop {
	private String sang;
	private int su;
	private int dan;
	
	public Shop(String sang, int su,int dan) {
		this.sang=sang;
		this.su=su;
		this.dan=dan;
	}
	public void setSang(String sang) {
		this.sang=sang;
	}
	public void setDan(int dan) {
		this.dan=dan;
	}
	public void setsu(int su) {
		this.su=su;
	}
	public String getSang()
	{
		return sang;
	}
	public int getSu() {
		return su;			
	}
	public int getDan() {
		return dan;
	}

	
}
