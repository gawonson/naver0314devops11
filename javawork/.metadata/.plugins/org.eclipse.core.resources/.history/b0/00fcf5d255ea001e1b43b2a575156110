package day0325;

public class Employee {
	//멤버변수
	private String name;
	private String position;
	private int age;
	private int gibon;
	private int sudang;
	
	
	public Employee(String name, String position, int age, int gibon, int sudang) {
		super();
		this.name = name;
		this.position = position;
		this.age = age;
		this.gibon = gibon;
		this.sudang = sudang;
	}


	public Employee(String name) {
		super();
		this.name = name;
	}
	
	
	
	
	
}
