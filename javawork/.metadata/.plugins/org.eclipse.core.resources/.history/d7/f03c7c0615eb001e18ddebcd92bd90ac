package day0326;
// 인터페이스는 음식점의 메뉴판과 같다.
//구현 가능한 목록을 나열만하고 실체(body)가 없기 때문
//상수와 추상메서드로만 구성이 되어있다.
interface InterA
{
	int MAX=10;// 상수이지만 구분할 필요가 없음으로 final 을 생략한다.
	public void disp();//추상메서드지만 구분 할 필요가 없으므로  abstract 를 생략한다.
	public void study();
}

class MyInter implements InterA
{
	@Override
	public void disp() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void study() {
		// TODO Auto-generated method stub
		
	}
}
public class Ex4_Interface {

}
